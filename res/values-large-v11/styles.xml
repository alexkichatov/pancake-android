<resources>    <style name="Theme.Pancake" parent="android:Theme.Holo.Light">        <item name="android:windowNoTitle">true</item>    </style>    <style name="Theme.Pancake.NoActionBar" parent="android:Theme.Holo.NoActionBar.Fullscreen"></style>    <style name="Theme.Pancake.Settings" parent="android:Theme.Holo.Light.Dialog.NoActionBar"></style>    <style name="detail_title">        <item name="android:layout_gravity">center_vertical</item>        <item name="android:layout_marginLeft">30dp</item>        <item name="android:paddingTop">10dp</item>        <item name="android:paddingBottom">10dp</item>        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>        <item name="android:textSize">32sp</item>        <item name="android:textColor">#3F434F</item>        <item name="android:textStyle">normal</item>    </style>    <style name="detail_list_item">        <item name="android:paddingLeft">0dp</item>    </style>    <style name="signin_button_style">        <item name="android:layout_marginTop">10dip</item>        <item name="android:textStyle">bold</item>        <item name="android:textColor">@color/sigin_color</item>        <item name="android:background">@drawable/siginin_selector</item>       <item name="android:paddingTop">10dip</item>        <item name="android:paddingBottom">10dip</item>    </style>    <style name="detail_header_text1">        <item name="android:maxLines">1</item>        <item name="android:paddingLeft">12dp</item>        <item name="android:paddingTop">30dp</item>        <item name="android:textSize">20sp</item>        <item name="android:textStyle">bold</item>    </style>    <style name="detail_header_text2">        <item name="android:maxLines">1</item>        <item name="android:paddingBottom">40dp</item>        <item name="android:paddingLeft">12dp</item>        <item name="android:paddingTop">10dp</item>        <item name="android:textSize">20sp</item>        <item name="android:textStyle">normal</item>    </style>    <style name="detail_list_image">        <item name="android:paddingLeft">10dp</item>        <item name="android:paddingTop">10dp</item>    </style>    <style name="edit_details">        <item name="android:layout_marginBottom">30dp</item>        <item name="android:layout_marginLeft">40dp</item>        <item name="android:layout_marginRight">40dp</item>        <item name="android:layout_marginTop">40dp</item>    </style>    <style name="menu_item">        <item name="android:clickable">true</item>        <item name="android:focusable">true</item>        <item name="android:gravity">center_vertical</item>        <item name="android:paddingBottom">15dp</item>        <item name="android:paddingLeft">15dp</item>        <item name="android:paddingTop">15dp</item>        <item name="android:paddingRight">15dp</item>        <item name="android:singleLine">true</item>        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>        <item name="android:textColor">@color/textcolor</item>    </style>    <style name="header_text_label">        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>        <item name="android:textColor">@android:color/black</item>        <item name="android:textStyle">bold</item>        <item name="android:layout_marginRight">5dip</item>        <item name="android:gravity">center_vertical</item>    </style>    <style name="detail_list_value">        <item name="android:maxLines">3</item>        <item name="android:paddingBottom">15dp</item>        <item name="android:paddingLeft">12dp</item>        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>        <item name="android:textSize">20sp</item>    </style>    <style name="detail_list_label">        <item name="android:paddingBottom">5dp</item>        <item name="android:paddingLeft">12dp</item>        <item name="android:paddingTop">15dp</item>        <item name="android:singleLine">true</item>        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>        <item name="android:textSize">16sp </item>    </style>    <style name="text_row_value">        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>        <item name="android:textStyle">normal</item>        <item name="android:layout_marginLeft">5dip</item>        <item name="android:layout_marginRight">10dip</item>        <item name="android:layout_marginTop">10dip</item>        <item name="android:paddingLeft">4dip</item>        <item name="android:paddingRight">7dip</item>        <item name="android:paddingTop">8dip</item>        <item name="android:textColor">@android:color/black</item>    </style></resources>